<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="groupId.JavaFX.MenuController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="218.0" prefHeight="400.0" prefWidth="200.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0">
         <children>
            <ImageView fitHeight="187.0" fitWidth="206.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/logo2.png" />
               </image>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#vocabScene" prefHeight="30.0" prefWidth="200.0" styleClass="btn" text="Vocabulary" />
            <Button fx:id="btn_translate" mnemonicParsing="false" onAction="#translateScene" prefHeight="30.0" prefWidth="200.0" styleClass="btn" text="Translate">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="200.0" styleClass="btn" text="Exit">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
